{
    "manifest_version": 3,
    "name": "ColorWizz",
    "description": "Fetches and categorizes colors from the current webpage.",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "wheel16.png",
        "64": "wheel64.png",
        "128": "wheel512.png"
      }
    }
  }
  